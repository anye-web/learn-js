//  Return Three times
 // function example (a, b){
 //  return function (c, d){
 //     return function (e,f){
 //       return a * c * e + b * d * f ;
 //     }
 //  }
 // }

const example = (a, b) => (c, d) => (e, f) => {  return a * c * e + b * d * f ; }

console.log(example(1,2)(3,4)(5,6))

example(1,2)(9,4)(5,6);
// 1 * 3 * 5 + 2 * 4 * 6 = 63

example(8,2)(6,4)(1,5);
// 8 * 6 * 1 + 2 * 4 * 5 = 88